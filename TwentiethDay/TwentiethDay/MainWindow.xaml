        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button Click="click_Registration" Content="Запись пациентов на приём" Width="250" Height="40" Margin="10"/>
            <Button Click="click_ListVisits" Content="Все приёмы" Width="250" Height="40" Margin="10"/>
        </StackPanel>
    </Grid>
</Window>
